{
  "name": "inu-play",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "=5.9.1"
  },
  "scripts": {
    "start": "node server.js",
    "test": "npm run test:integration",
    "lint": "standard",
    "test:integration": "chimp",
    "test:components": "browserify test/components/**.js | tap-closer | smokestack | tap-spec",
    "build": "browserify client.js -o public/bundle.js -dv",
    "dev:watch": "watchify client.js -o public/bundle.js -dv | npm run livereload",
    "livereload": "wtch -d public | garnish --level debug",
    "dev:serve": "DEBUG=* node-dev server.js",
    "dev": "NODE_ENV=development npm-run-all -p dev:*"
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "evalify",
        {
          "files": [
            "config.js"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simontegg/inu-play.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simontegg/inu-play/issues"
  },
  "homepage": "https://github.com/simontegg/inu-play#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "deep-merge": "^1.0.0",
    "defined": "^1.0.0",
    "domready": "^1.0.8",
    "global": "^4.3.0",
    "inu": "^2.3.1",
    "is-array": "^1.0.1",
    "is-html": "^1.0.0",
    "lodash": "^4.13.1",
    "muxrpc": "^6.3.3",
    "parse-url": "^1.3.3",
    "parseurl": "^1.3.1",
    "pull-many": "^1.0.6",
    "pull-notify": "^0.1.0",
    "pull-pushable": "^2.0.0",
    "pull-serializer": "^0.3.2",
    "pull-stream": "^3.4.0",
    "reselect": "^2.5.1",
    "serve-static": "^1.10.3",
    "sheet-router": "^3.1.0",
    "simple-rc": "^1.0.3",
    "stream-to-pull-stream": "^1.6.10",
    "supertest": "^1.2.0",
    "tape": "^4.6.0",
    "tcomb": "^3.2.1",
    "ws": "^1.1.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-lodash": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chimp": "^0.35.0",
    "cucumber-assert": "^1.0.4",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "garnish": "^5.2.0",
    "is-element": "^0.1.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^2.2.2",
    "standard": "^7.1.2",
    "tap-closer": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "watchify": "^3.7.0",
    "wtch": "^4.0.1",
    "yo-yoify": "^2.0.1"
